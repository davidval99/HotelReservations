s.room = data.body;\r\n\r\n        });\r\n\r\n        this.$http.get('http://www.json-generator.com/api/json/get/bVKSVzMguq?indent=2').then(function(data){\r\n                    this.hotels = data.body;\r\n                });\r\n   },\r\n}\r\n\r\n</script>\r\n\r\n<style scoped>\r\ntable, th, td {\r\n  border: 1px solid black;\r\n  border-collapse: collapse;\r\n}\r\nth, td {\r\n  padding: 5px;\r\n  text-align: left;\r\n}\r\n</style>"]}]}